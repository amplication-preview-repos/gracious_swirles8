datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Route {
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  updatedAt DateTime @updatedAt
  uri       String?
}

model Predicate {
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  path      String?
  updatedAt DateTime @updatedAt
}

model Filter {
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  name      String?
  updatedAt DateTime @updatedAt
  value     String?
}
